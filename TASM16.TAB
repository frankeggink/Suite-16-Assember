/* Register OPS- */

/* 1n */
SET R0,*      1000 4 SWAP 1
SET R1,*      1100 4 SWAP 1
SET R2,*      1200 4 SWAP 1
SET R3,*      1300 4 SWAP 1
SET R4,*      1400 4 SWAP 1

SET R14,*     1E00 4 SWAP 1

/* 2n */
LD AC,R1      21 2 NOP 1
LD AC,R2      22 2 NOP 1
LD AC,R3      23 2 NOP 1
LD AC,R4      24 2 NOP 1

/* 3n */
