ST R4, AC     34 2 NOP 1

ST R6, AC     36 2 NOP 1

/* 4n */
LD AC,@R1     41 2 NOP 1
LD AC,@R2     42 2 NOP 1
LD AC,@R3     43 2 NOP 1
LD AC,@R4     44 2 NOP 1

/* 5n */
/*  todo */
ST @R1,AC
ST @R2,AC
ST @R3,AC
ST @R4,AC


